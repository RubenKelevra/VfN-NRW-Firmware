config hotspot 'hotspot'
	option community ''
	option wlanap '1'
	option wlanmesh '1'

config ap 'private'
	option ssid ''
	option key ''

config firmware 'fw'
	option confver '0'
	option distribution 'stable'
	option autoupgrade '1'

config interface 'vpn'             
       option enabled '0'   
       option if 'vpn-ffmesh'    
       option limit_out '500'                  
       option limit_in '2000'
